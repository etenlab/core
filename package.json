{
  "name": "@eten-lab/core",
  "version": "0.1.10",
  "description": "crowd bible core services",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etenlab/core.git"
  },
  "keywords": [
    "eten-lab",
    "core",
    "crowd.bible"
  ],
  "author": "eten-lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/etenlab/core/issues"
  },
  "homepage": "https://github.com/etenlab/core#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "module": "dist/core.esm.js",
  "size-limit": [
    {
      "path": "dist/core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/core.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/language-tags": "^1.0.1",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@eten-lab/models": "^1.3.12",
    "@eten-lab/ui-kit": "^0.5.42",
    "axios": "^1.3.6",
    "localforage": "^1.10.0",
    "roarr": "^7.15.0",
    "typeorm": "^0.3.12",
    "yup": "^1.1.1"
  }
}
